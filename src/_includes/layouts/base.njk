{% set assetHash = global.random() %}

<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="X-UA-Compatible" content="ie=edge" />
    {% include "partials/meta-info.njk" %}
    <link rel="alternate" type="application/rss+xml" href="{{ site.url }}/feed.xml" />
    <link rel="stylesheet" media="print" href="/css/style.css?{{ assetHash }}" onload="this.media='all'" />
    {# <link rel="stylesheet" href="/css/{{ cssFile }}?{{ assetHash }}" /> #}
    <script defer src="https://identity.netlify.com/v1/netlify-identity-widget.js"></script>
    {% eleventyGoogleFonts 'https://fonts.googleapis.com/css2?family=Gabarito:wght@400;600&display=swap' %}
    
    {# {% set css %}
        {% include "css/critical.css" %}
    {% endset %}
    <style>
    {{ css | cssmin | safe }}
    </style> #}
    {% block head %}{% endblock %}

    {% eleventyGoogleFonts 'https://fonts.googleapis.com/css2?family=Gabarito:wght@400;600&display=swap' %}

  </head>
  <body>
    <div class="site-wrap">
      {% include "partials/header.njk" %}
    
      <main tabindex="-1" id="main-content">
        {% block content %}{% endblock %}
      </main>
    

    </div>
    
  </body>
</html>